{
    "name": "Jobdude",
    "version": "1.0.0",
    "description": "This is a demo chrome extension for Personarise",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["script.js"],
            "css": ["style.css"]
        }
    ],
    "permissions": [
      "cookies",
      "storage",
      "tabs"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    }
}